def readtxt(fil):
    from operator import itemgetter
    text = fil.read()
    lineArray = text.split("\n")
    Masterlist = []
    for i in range(len(lineArray)):
        if lineArray[i] == "<tr>":
            dic ={}
            countyWithTag = lineArray[i+1]
